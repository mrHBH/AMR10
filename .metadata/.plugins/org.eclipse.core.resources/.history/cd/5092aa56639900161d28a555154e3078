package parkingRobot.hsamr0.Navigation;

import lejos.geom.Point;
import lejos.robotics.navigation.Pose;

import java.util.LinkedList;
import java.util.List;

public class StraightParking extends Straight {
	private List<ParkingSlot> parkingSlots = new LinkedList<ParkingSlot>();
	private Point parkingZoneStart;
	private Point parkingZoneEnd;
	
	public StraightParking(Pose startPose, Point parkingZoneStart, Point parkingZoneEnd){
		super(startPose);
		this.parkingZoneStart = parkingZoneStart;
		this.parkingZoneEnd = parkingZoneEnd;
	}

	@Override
	public boolean hasParkingZone() {
		return true;
	}
}
